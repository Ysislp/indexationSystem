{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
      {
          "environment": [
              {
                  "name": "MYSQL_HOST",
                  "value": "docker-mysql"
              },
              {
                  "name": "MYSQL_ROOT_PASSWORD",
                  "value": "root"
              },
              {
                  "name": "MYSQL_DATABASE",
                  "value": "db_news"
              }
          ],
          "essential": true,
          "memoryReservation": 256,
          "image": "mysql:5.7",
          "mountPoints": [
              {
                  "containerPath": "/var/app/current/sql-spring",
                  "sourceVolume": "Sql-Spring"
              }
          ],
          "name": "docker-mysql",
          "portMappings": [
              {
                  "containerPort": 3306,
                  "hostPort": 3306
              }
          ]
      },
      {
          "environment": [
              {
                  "name": "MYSQL_HOST",
                  "value": "docker-mysql"
              },
              {
                  "name": "MYSQL_ROOT_PASSWORD",
                  "value": "root"
              },
              {
                  "name": "MYSQL_DATABASE",
                  "value": "db_news"
              },
              {
                  "name": "INSTANCE_ID",
                  "value": "prueba1"
              }
          ],
          "essential": false,
          "memoryReservation": 128,
          "image": "bc01.dev.lab.plugbox.io:8085/${REPOSITORY}/${ARTIFACT_ID}:${VERSION}",
          "name": "indexationsystem1"
      },
      {
          "environment": [
              {
                  "name": "MYSQL_HOST",
                  "value": "docker-mysql"
              },
              {
                  "name": "MYSQL_ROOT_PASSWORD",
                  "value": "root"
              },
              {
                  "name": "MYSQL_DATABASE",
                  "value": "db_news"
              },
              {
                  "name": "INSTANCE_ID",
                  "value": "prueba2"
              }
          ],
          "essential": false,
          "memoryReservation": 128,
          "image": "bc01.dev.lab.plugbox.io:8085/${REPOSITORY}/${ARTIFACT_ID}:${VERSION}",
          "name": "indexationsystem2"
      },
      {
          "essential": true,
          "memoryReservation": 256,
          "image": "nginx:latest",
          "mountPoints": [
              {
                  "containerPath": "/etc/nginx/conf.d",
                  "sourceVolume": "_Nginx"
              }
          ],
          "name": "nginx",
          "portMappings": [
              {
                  "containerPort": 80,
                  "hostPort": 80
              }
          ]
      }
  ],
  "family": "",
  "volumes": [
      {
          "host": {
              "sourcePath": "/var/app/current/sql-spring"
          },
          "name": "Sql-Spring"
      },
      {
          "host": {
              "sourcePath": "/var/app/current/nginx"
          },
          "name": "_Nginx"
      }
  ]
}